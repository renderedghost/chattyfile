{
  "name": "Chatty File Uploader",
  "version": "0.0.2",
  "manifest_version": 3,
  "description": "Upload larger text files to ChatGPT",
  "action": {},
  "icons": {
    "16": "chatty-16.png",
    "48": "chatty-48.png",
    "128": "chatty-128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://chat.openai.com/*"],
      "js": ["content.js"]
    }
  ]
}